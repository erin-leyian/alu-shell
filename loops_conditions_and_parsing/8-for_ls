#!/usr/bin/env bash
# This script lists files/directories showing only the part after the first dash

for file in *; do
  # Check if the name contains a dash
  if [[ "$file" == *-* ]]; then
    # Extract the part after the first dash
    echo "${file#*-}"
  else
    # If no dash, print the full name as is
    echo "$file"
  fi
done
